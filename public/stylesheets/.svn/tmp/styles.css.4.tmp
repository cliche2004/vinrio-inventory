body {
	/*margin:0 auto;*/	
	font-size: 12px;
	font-family: Verdana,Arial,sans-serif;	
}

a img {
	border: 0;	
}

.site-container {
	margin:0px auto;
}

.wrap {
	/*margin:15px;*/
	margin:0px auto;
	width:1024px;
}

.curved-all {
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	behavior:url(border-radius.htc);
	border: 1px solid #00a2ff;
}

#header {
	width:1024px;
	/*height: 87px;*/
}

.header-name {
	margin:3px;
	/*border: 1px solid #000000;*/
	-moz-border-radius: 5px 5px 0px 0px;
	background-color: #006ba2;
	height: 40px;
}

.header-name #company-name {
	margin: 15px 10px;
	color: #FFFFFF;
	font-family:Arial;
	font-weight: bold;
	font-size: 14px;
	float:left;
	width:300px;
} 

.header-name #user-logged {
	margin: 15px 10px;
	color: #FFFFFF;
	font-family:Arial;
	font-weight: normal;
	font-size: 12px;
	float:right;
	width: 300px;
	text-align: right;
}

#user-logged a {
	color:white;
	text-decoration: none;
	cursor: pointer;
}

#header .logo {
	margin:0;
	display:block;
	letter-spacing:0;
	width: 300px;
	padding: 10px;
	float:left;
	
}

.header-tab {
	height: 40px;
	-moz-border-radius:0px 0px 5px 5px;
	-webkit-border-radius: 5px;
	behavior:url(border-radius.htc);
	/*background: #d0ebfb;*/
	margin: 3px;
	
}

.header-tab #tab-holder {
	margin:0px;
	height: 40px;
	padding: 10px 0px 0px 10px;
}

#tab-holder .tab-button {
	float:left;
	height: 20px;
	width: 146px;
	background-image: url(images/buttons/tab1.png);
	margin-right: 20px;
}

.tab-button #tab-title {
	font-size: 14px;
	font-family: Arial;
	color:black;
	padding: 2px 25px;
	cursor: pointer;
}

#tab-title a {
	cursor: pointer;
	text-decoration: none;
	color: #000000;
} 

#content {
	width:1024px;
	margin: 10px 0px;
	
}

.border-filler{
	background: transparent url(../images/line-border-filler.png) repeat-x;
	height: 3px;
	margin: 2px 0;
}

.menu-content {
	/*width:310px; */
	width:230px;
	float:left;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	behavior:url(border-radius.htc);
	border: 1px solid #00a2ff;
	min-height: 600px;	
}

.menu-content #menu-header {
	height: 40px;
	-moz-border-radius: 5px 5px 0px 0px;
	-webkit-border-radius: 5px;
	behavior:url(border-radius.htc);
	background: #2c9bda;
	margin: 3px;
}

#menu-header .menu-header-title {
	padding: 10px 0px 0px 20px;
	font-family: Arial;
	font-size: 14px;
	font-weight: bold;
}

#menu-header .menu-list {
	min-height: 552px;
	background: #d0ebfb;
	margin-top: 16px;
	-moz-border-radius:0px 0px 5px 5px;
}

.accordion-menu {
	float:left;
	width: 218px;
	margin: 5px 2px 0px 2px;
}

.accordion-menu #accordion div p {
	font-size:12px;
	
}

.accordion-menu #accordion div p img{
	padding: 10px;
}

.accordion-menu #accordion div div {
	font-size: 12px;
}
.accordion-menu #accordion div div ul {
	list-style: none;
	margin: 0px;
	padding: 0px;
		
}

.accordion-menu #accordion div div ul li{
	font-size: 12px;
	text-decoration: none;
	height: 30px;
	background: #FFFFFF;
	border-bottom: 1px solid #CDC3B7;
	cursor: pointer;
}

.accordion-menu #accordion div div ul li div{
	height: 30px;
	padding: 8px 0px 0px 25px;
}

.accordion-menu #accordion div div ul li div a{
	text-decoration: none;
}

/*Contents*/


.main-content {
	margin-left: 10px;
	float: left;
	
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	behavior:url(border-radius.htc);
	border: 1px solid #00a2ff;
	
	width:780px;
	overflow: hidden;
	min-height: 600px;
}

.main-content #view-inner-content {
	border: 1px solid #990000;
	/*width: 1059px;*/
	
	margin: 3px;
	height: 40px;
	
	-moz-border-radius: 5px 5px 0px 0px;
	-webkit-border-radius: 5px;
	behavior:url(border-radius.htc);
	border: 1px solid #00a2ff;
	background: #2c9bda;
	
	font-family: "Arial";
	font-size: 14px;
	color:#FFFFFF;
}

#view-inner-content img {
	padding: 10px;
	float:left;
}

#view-inner-content .breadcrumb {
	margin:0px;
	padding-top:12px;
	float:left;
}

.main-content #content-container {
	margin: 3px;
}

/*table listing*/

.view-content-list {
	margin-top:25px;
}

.view-content-list #list-title, #list-title{
	width:350px;
	height: 25px;
	-moz-border-radius:5px 5px 0px 0px;
	-webkit-border-radius:10px;
	behavior:url(border-radius.htc);
	background: #9dd8fb;
	float:left;
	margin-left: 1px;
}

#list-title img {
	padding: 5px 0px 0px 10px;
	float:left;
}

#list-title .list-title-text {
	font-family: "Arial";
	font-size: 12px;
	font-weight: bold;
	color: #204b64;
	width: 200px;
	float: left;
	padding: 5px 0px 0px 10px;
}

.view-content-list #buttons{
	float:left;
	margin-left: 10px;
}

.view-content-list #list-content {
	
	margin-top: 1px;
}

#list-content table {
	margin:0px;
	padding: 0px;
	border: 2px solid #d0ebfb;
	
}

#list-content table tr th {
	width:120px;
	font-family:"Arial";
	font-size: 14px;
	border-right: 1px solid #d0ebfb;
	text-align: left;
}

#list-content table tr td {
	width:120px;
	font-family:"Arial";
	font-size: 12px;
	border-top: 1px solid #d0ebfb;
	border-right: 1px solid #d0ebfb;
	text-align: left;
}

/*end*/

/*login styles*/
.login-container {
	margin-left:40px;
	margin-top: 10px;
}

.txtfield-username {
	-moz-border-radius:9px 9px 9px 9px;
	-webkit-border-radius:10px;
	behavior:url(border-radius.htc);
	border: 1px solid #000000;
	float:left;
	padding: 5px; 
	width:210px;
	background: #e5f5fe;
	height: 25px;
}

.txtfield-password {
	-moz-border-radius:9px 9px 9px 9px;
	-webkit-border-radius:10px;
	behavior:url(border-radius.htc);
	border: 1px solid #000000;
	float:left;
	padding: 5px; 
	width:140px;
	background: #e5f5fe;
	height: 25px;
}

/*end*/

/*table listing*/
#hor-minimalist-b
{
	font-family: Arial;
	font-size: 12px;
	background: #fff;
	width: 773px;
	border-collapse: collapse;
	text-align: left;
	border-left: 1px solid #ededed;
	border-right: 1px solid #ededed;
}

#hor-minimalist-b tr.header {
	background: url(images/tr-bg.png);
	background-repeat: repeat-x;
		border-left: 1px solid #ededed;
	border-right: 1px solid #ededed;
}

#hor-minimalist-b tr.odd {
	background-color: #ebebeb;
}

#hor-minimalist-b th
{
	font-size: 12px;
	font-weight: normal;
	/*color: #039;*/
	color:white;
	padding: 6px 8px;
}
#hor-minimalist-b td
{
	border-bottom: 1px solid #ccc;
	/*color: #669;*/
	padding: 6px 8px;
}

#hor-minimalist-b td a {
	text-decoration: none;
	cursor:pointer;
	color: #669;
	padding: 6px 8px;
}

#hor-minimalist-b tbody tr:hover td, #hor-minimalist-b tbody tr:hover td a:hover
{
	color: #009;
}

