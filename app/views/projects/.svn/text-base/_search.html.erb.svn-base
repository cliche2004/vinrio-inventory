<div>
	<%= text_field_tag :user, @user %>
</div>
<script>
  
  $('#user').keyup(function(event) { 
   
        jQuery.ajax({
      	  url: '/projects/search_users',
      	  type: 'POST',
      	  data: { user: $('#user').val(), project_id: <%= @project.id %> },
      	  success: function(data) {}
      	})

    })

</script>
