	<div class="headtitle">
			<div class="headtxt">Account Listing</div>
		</div>
		<div >
			<table class="listing" width="100%" cellspacing=0 cellpadding=0>
				<thead class="lhead">
					<th width="25px" align='center'>ID</th>
					<th>Name</th>
					<th>Account</th>
					<th>Type</th>
					<th></th>
				</thead>
				<tbody>
					<% for user in @users do %>
						<tr class="<%= cycle('odd','')%>">					
							<td><%= user.id %></td>
							<td><%= user.name %></td>
							<td><%= user.login %></td>
							<td><%= user.type %></td>
							<td style="float:right;">
							<span>
			 	             	<%= link_to_remote image_tag('icons/edit.gif',:title=>'Update' ),
			 	             	                    :url => {:action => 'ajax_edit', :id => user.id } %>
                              	<%= link_to_remote image_tag('icons/delete.png',:title=>'Delete' ),
			 	             	                    :url => {:action => 'ajax_delete', :id => user.id }, 
													:confirm => 'Are you sure to delete?' %>
			                </span>
							</td>
						</tr>				
					<%end%>
				</tbody>
			</table>
			
			
				<div class="list-footer"></div>
		
	</div>
